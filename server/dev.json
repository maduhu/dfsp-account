{
  "implementation": "dfsp-account",
  "console": {
    "port": 30009,
    "server": {
      "host": "0.0.0.0",
      "port": 30009
    }
  },
  "stdOut": {
    "useColor": false,
    "mode": "long"
  },
  "log": {
    "streams": [
      {
        "level": "trace",
        "stream": "../logRotateStream",
        "streamConfig": {
          "path": "ut5_%Y-%m-%d.log",
          "symlink": "ut5.log",
          "compress": true
        }
      }
    ]
  },
  "masterBus": {
    "logLevel": "debug",
    "socket": "dfsp-account"
  },
  "workerBus": {
    "logLevel": "debug"
  },
  "db": {
    "logLevel": "trace",
    "db": {
      "host": "postgres"
    },
    "permission": {
      "requireSession": true
    }
  },
  "script": {
    "#mock": true
  },
  "debug": true
}
